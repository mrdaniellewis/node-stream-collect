{
  "name": "stream-collect",
  "version": "2.0.0",
  "description": "Collects the output of a stream",
  "keywords": [
    "stream",
    "collect",
    "promise"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js",
    "jest": "jest",
    "test": "npm run lint && npm run jest"
  },
  "dependencies": {
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1"
  },
  "author": "Daniel Lewis",
  "license": "ISC",
  "repository": "https://github.com/mrdaniellewis/node-stream-collect.git",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=6.9.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "func-names": "off",
      "max-len": [
        "error",
        120,
        2
      ],
      "no-cond-assign": [
        "error",
        "except-parens"
      ],
      "no-param-reassign": "off",
      "no-underscore-dangle": "off"
    }
  }
}
